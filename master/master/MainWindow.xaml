<Window x:Class="master.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:master"
        mc:Ignorable="d"
        Title="Главная страница" Height="800" Width="1200">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        
        <Grid Grid.Row="0" Background="#FF67BA80">
            <Label Content="Мастер пол" HorizontalAlignment="Center"
                   VerticalAlignment="Center"></Label>
            <Image Source="/Resources/Мастер пол.png" HorizontalAlignment="Left"
                   Margin="10" />
        </Grid>
        
        
        <Frame Grid.Row="1" x:Name="MainFrame"
               NavigationUIVisibility="Hidden">
        </Frame>
    </Grid>
</Window>
